   "source": [
    "def histogram(image):\n",
    "    # Calculate histogram\n",
    "    histogram = np.zeros(256, dtype=int)\n",
    "    height, width = image.shape\n",
    "    for i in range(height):\n",
    "        for j in range(width):\n",
    "            pixel_value = image[i, j]\n",
    "            histogram[pixel_value] += 1\n",
    "\n",
    "    return histogram"
   ]
  }
