    "def divideImage(new_img):\n",
    "    for x in range(new_img.shape[0]):\n",
    "        for y in range(new_img.shape[1]):\n",
    "            new_pixel_value = new_img[x][y] / 2\n",
    "            new_img[x][y] = np.minimum(new_pixel_value, 255)\n",
    "    \n",
    "    return new_img"
